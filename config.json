{
  "name": "Optymalizator PV",
  "version": "0.1.0",
  "slug": "pv_optimizer",
  "description": "Dodatek do Home Assistant optymalizujący pracę systemu PV.",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "startup": "application",
  "boot": "auto",
  "options": {
        "log_level": "info"
  },
  "schema": {
        "log_level": "list(debug|info|warning|error|critical)?"
  },
  "ports": {
    "8123/tcp": 8123
  },
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8123]/pv_optimizer",
  "ingress": true,
  "ingress_port": 8123,
    "ingress_entry": "/pv_optimizer",
  "panel_icon": "mdi:solar-power",
  "panel_admin": false,
  "watchdog": "[PROTO:ssl]://[HOST]:[PORT:8123]/api/health",
  "image": "gpietrzakpl/pv_optimizer:{arch}"
}